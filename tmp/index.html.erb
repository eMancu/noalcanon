<h1>Listing reclamos</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Nombre</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @reclamos.each do |reclamo| %>
  <tr>
    <td><%= reclamo.email %></td>
    <td><%= reclamo.nombre %></td>
    <td><%= link_to 'Show', reclamo %></td>
    <td><%= link_to 'Edit', edit_reclamo_path(reclamo) %></td>
    <td><%= link_to 'Destroy', reclamo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Reclamo', new_reclamo_path %>
